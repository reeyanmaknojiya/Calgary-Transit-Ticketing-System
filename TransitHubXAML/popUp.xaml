        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TransitHubXAML"
        mc:Ignorable="d"
        Title="popUp" Height="200" Width="200">
    <Grid>
        <Label Name="_label"  HorizontalContentAlignment="Center" Content="Hello world!" Margin="12,19,10,99"/>
        <Button Click="goBack_Click"  Foreground="White"  Background="#B83A3F" Name="goBack" Content="Back" Margin="10,82,109,33"></Button>
